{
    "name": "@swiss-army-knife/services",
    "version": "1.1.0",
    "description": "",
    "author": "Matthias Einbrodt",
    "license": "MIT",
    "keywords": [],
    "main": "./dist/cjs/index.js",
    "types": "./dist/cjs/types/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/types/index.d.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/cjs/types/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        }
    },
    "files": [
        "./dist/**/*"
    ],
    "scripts": {
        "build": "npm run build:cjs && npm run build:esm",
        "build:cjs": "npm run delete:buildDir:cjs && tsc --project tsconfig.cjs.json",
        "build:esm": "npm run delete:buildDir:esm && tsc --project tsconfig.esm.json",
        "delete:buildDir:cjs": "del-cli --force ./dist/cjs",
        "delete:buildDir:esm": "del-cli --force ./dist/esm",
        "test": "jest"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^18.15.0",
        "del-cli": "^5.0.0",
        "jest": "^29.6.4",
        "ts-jest": "^29.1.1",
        "tslib": "^2.6.2",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@jest/globals": "^29.6.4",
        "@swiss-army-knife/models": "^1.3.2",
        "@swiss-army-knife/utilities": "^1.4.0",
        "csv-parse": "^5.3.6",
        "csv-stringify": "^6.3.0",
        "fast-xml-parser": "^4.2.7",
        "fs-extra": "^11.1.0",
        "ts-results": "^3.3.0"
    }
}